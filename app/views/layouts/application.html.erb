<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title><%=	@page_title  %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <link href="https://fonts.googleapis.com/css?family=Lato|Roboto+Slab" rel="stylesheet">

    <meta name="keywords" content="<%= @seo_keywords %>"/>

    <%=	 favicon_link_tag 'favicons/favicon-es-new.ico'  %>

    <link rel="apple-touch-icon" href="apple-touch-icon-iphone-60x60.png">
    <link rel="apple-touch-icon" sizes="60x60" href="apple-touch-icon-ipad-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="apple-touch-icon-iphone-retina-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="apple-touch-icon-ipad-retina-152x152.png">

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

          <%=	 render "shared/application_nav" %>

          <%= yield %>



          <%=	 render "shared/application_footer" %>

          <%=  alerts %>

          <script src="jquery.js"></script>
          <script src="jquery.sticky.js"></script>
          <script>
              $(document).ready(function () {
                  $("#sticky-h").sticky({topSpacing: 0});
              });
          </script>
  </body>
</html>